{
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["*.json", ".prettierrc"],
      "options": {
        "parser": "json",
        "tabWidth": 2
      }
    },
    {
      "files": ["*.md"],
      "options": {
        "tabWidth": 2
      }
    }
  ],
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "none"
}
